{
    "version": 2,
    "name": "coffee-shop-app",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "installCommand": "npm install",
                "buildCommand": "npx expo export:web --platform web"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ],
    "outputDirectory": "dist"
}