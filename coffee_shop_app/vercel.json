{
    "version": 2,
    "name": "coffee-shop-app",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "installCommand": "npm install",
                "buildCommand": "npx expo export:web --target web --output-dir public"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "outputDirectory": "public"
}