{
    "version": 2,
    "name": "coffee-shop-app",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "outputDirectory": "public"
}